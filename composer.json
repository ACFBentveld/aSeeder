{
    "name": "acfbentveld/shop",
    "description": "A laravel webshop",
    "keywords": [
        "shop", "webshop", "ecommerce","laravel"
    ],
    "homepage": "https://github.com/ACFBentveld/Shop",
    "license": "MIT",
    "version": "0.0.21",
    "authors": [
        {
            "name": "Wim Pruiksma",
            "email": "wim@acfbentveld.nl",
            "homepage": "https://acfbentveld.nl/",
            "role": "Lead developer"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        
    },
    "autoload": {
        "psr-4": {
            "ACFBentveld\\Shop\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ACFBentveld\\Shop\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "ACFBentveld\\Shop\\ShopServiceProvider"
            ],
            "aliases": {
                "Shop": "ACFBentveld\\Shop\\ShopFacade"
            }
        }
    }
}
